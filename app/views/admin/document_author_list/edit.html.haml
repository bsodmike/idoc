- form_for(:document_author_list, :url => admin_document_author_list_path, :html => { :method => :put }) do |f|
  != f.label :add_document_authors
  != f.collection_select :add_document_authors, @users, :id, :email, {}, {:multiple => "multiple"}
  != f.label :remove_document_authors, "Remove document authors"
  != f.collection_select :remove_document_authors, @authors, :id, :email, {}, {:multiple => "multiple"}
  != submit_tag "Update document authors"